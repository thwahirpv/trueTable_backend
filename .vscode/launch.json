{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Convex Functions",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/convex/functions/restaurants.ts",
      "outFiles": ["${workspaceFolder}/convex/_generated/**/*.js"],
      "preLaunchTask": "npm: build",
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}